{
  "extraKnownMarketplaces": {
    "claude-toolkit": {
      "source": {
        "source": "git",
        "url": "https://github.com/r0227n/claude-toolkit.git",
        "ref": "feature/parallel-dev",
        "path": "plugins/parallel-dev"
      }
    }
  },
  "enabledPlugins": {
    "parallel-dev@claude-toolkit": true
  },
  "permissions": {
    "allow": [
      "Edit",
      "Write",
      "Bash(cat:*)",
      "Bash(ls:*)",
      "Bash(tree:*)",
      "Bash(gh pr create:*)",
      "Bash(gh pr view:*)",
      "Bash(gh run view:*)",
      "Bash(git -C:* add:*)",
      "Bash(git -C:* branch:*)",
      "Bash(git -C:* checkout:*)",
      "Bash(git -C:* commit:*)",
      "Bash(git -C:* diff:*)",
      "Bash(git -C:* log:*)",
      "Bash(git -C:* merge:*)",
      "Bash(git -C:* push:*)",
      "Bash(git -C:* status:*)",
      "Skill(ui-ux-pro-max)",
      "Bash(python3 **/*.claude/skills/ui-ux-pro-max/scripts/*)",
      "Bash(mise:*)",
      "Bash(swift package:*)",
      "Bash(xcodebuild:*)",
      "Bash(timeout 30 xcodebuild test:*)",
      "Bash(timeout 60 xcodebuild test:*)",
      "Bash(swift test:*)",
      "Bash(swift -e:*)",
      "Bash(log show:*)",
      "Bash(xcrun xcresulttool:*)",
      "Bash(pkill:*)",
      "Bash(killall:*)",
      "Bash(pgrep:*)",
      "Bash(xargs:*)",
      "Bash(open:*)",
      "Bash(codesign:*)"
    ],
    "defaultMode": "plan"
  },
  "hooks": {
    "PostToolUse": [
      {
        "matcher": "Edit|Write",
        "hooks": [
          {
            "type": "command",
            "command": "\"$CLAUDE_PROJECT_DIR\"/scripts/format-lint.sh"
          }
        ]
      }
    ]
  }
}
